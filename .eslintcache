ours + \"h \" + minutes + \"m \" + seconds + \"s\";\n  }\n  forcerender = () => {\n    setInterval(() => {\n      this.setState({\n        fl: !this.state.fl,\n      });\n    }, 3000);\n  };\n  renderItems() {\n    const data = this.state.auctions;\n    let ind = 0;\n    const maxbids = this.state.lists;\n    return (\n      <Row>\n        {data.map((item, index) => {\n          let max = 0;\n          return (\n            <Row className=\"xsmall\" key={index}>\n              <Col>\n                <img className=\"cover\" src={item.auction_images[0].path} />\n              </Col>\n              <Col>\n                {item.auction_categories_id === 1 ? (\n                  <i>Residential</i>\n                ) : item.auction_categories_id === 2 ? (\n                  <i>Commercial</i>\n                ) : item.auction_categories_id === 3 ? (\n                  <i>Industrial</i>\n                ) : (\n                  <i>Any</i>\n                )}\n                <br />\n                {item.type}\n              </Col>\n              <Col>\n                <i>{item.area} sqft </i>\n                <br />\n                {this.rendarTimeLaps(item)}\n              </Col>\n              <Col>\n                <CountBids id={item.id} />\n                bids <br />\n                {maxbids.map((i, ind) => {\n                  if (i.item_id == item.id)\n                    if (i.price > max) {\n                      max = i.price;\n                      win = i.username;\n                      ind = ind;\n                    }\n                })}\n                <h6 style={{ marginLeft: \"5px\" }} id={ind}>\n                  {\" \"}\n                  $ {this.numberWithCommas(max)}\n                </h6>\n              </Col>\n            </Row>\n          );\n        })}\n      </Row>\n    );\n  }\n  renderRecoms() {\n    let ind = 0;\n    const maxbids = this.state.lists;\n    const recom=this.state.recoms;\n  if(recom.length>0){\n    return (\n      <Row>\n