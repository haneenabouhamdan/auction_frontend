ion\" placeholder=\"Expiration date\" />\n                    <label className=\"message\">{ this.state.expiration_date_error}</label>\n                    </Row>\n                    <Row style={{marginTop:\"10px\"}}>\n                    <Input type=\"number\" onChange={this.handleChange} value={this.state.cvv} name=\"cvv\" placeholder=\"cvv\" />\n                    <label className=\"message\">{ this.state.cvvError}</label>\n                    </Row>\n                    <Row style={{marginTop:\"10px\",textAlign:\"center\"}}>\n                    <button onClick={this.onSubmit} style={{backgroundColor:\"rgb(18, 165, 140)\",color:\"white\",border:\"0\",width:\"100px\",height:\"40px\"}}>Submit</button>\n                    </Row>\n                </CardBody>\n            </Card>\n            </div>\n        )\n    }\n}\nexport default PaymentDetails;","/home/haneen/dpm_frontend/src/Components/WelcomePage.js",["177","178","179","180","181","182","183","184","185","186","187","188","189","190","191","192","193","194","195","196","197"],"import React from \"react\";\nimport Navbar from \"../Components/Navbar\";\nimport CountBids from \"../Components/CountBids\";\nimport welcome from \"../images/welcome.jpeg\";\nimport w1 from \"../images/w1.jpeg\"\nimport { MDBIcon } from \"mdbreact\";\nimport \"../style/MyAuctions.css\";\nimport MainCategories from \"./MainCategories\";\nimport Near from \"../Components/Near\";\nimport firebase from \"../utils/firebase\";\nimport { Button, Card, CardHeader, CardBody, Row, Col } from \"reactstrap\";\nimport axios from \"axios\";\nlet win = \"\";\nclass welcomePage extends React.Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      user_id: 0,\n      auctions: [],\n      lists: [],\n      fl: true,\n      ttype: \"\",\n      num: 0,\n      last_name: \"\",\n      first_name: \"\",\n      itemslist: [],\n      category: 0,\n      longitude: \"a\",\n      latitude: \"a\",\n      area_min: \"a\",\n      area_