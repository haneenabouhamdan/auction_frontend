der=\"Description\"\n                />\n                <DropdownButton\n                  title=\"Land Type\"\n                  id=\"type\"\n                  onSelect={this.handleSelect}\n                >\n                  <Dropdown.Item eventKey=\"1\">Residential</Dropdown.Item>\n                  <Dropdown.Item eventKey=\"2\">Commercial</Dropdown.Item>\n                  <Dropdown.Item eventKey=\"3\">Industrial</Dropdown.Item>\n                  <Dropdown.Item eventKey=\"4\">Agricultural</Dropdown.Item>\n                  <Dropdown.Item eventKey=\"5\">Others</Dropdown.Item>\n                </DropdownButton>\n\n                <div className=\"img images\">\n                  <ImageUploading\n                    onChange={this.onChange}\n                    maxNumber={maxNumber}\n                    acceptType={[\"jpg\", \"png\", \"jpeg\", \"webp\"]}\n                    id=\"img\"\n                    multiple\n                  >\n                    {({ imageList, onImageUpload }) => (\n                      // write your building UI\n                      <div className=\"imageuploader\">\n                        <div className=\"mainBtns\">\n                          <button\n                            className=\"btn btn-primary mr-1\"\n                            onClick={onImageUpload}\n                          >\n                            Upload Images\n                          </button>\n                        </div>\n                        {imageList.map((image) => (\n                          <div className=\"imagecontainer\" key={image.key}>\n                            <img src={image.dataURL} />\n                          </div>\n                        ))}\n                      </div>\n                    )}\n                  </ImageUploading>\n                </div>\n                <label className=\"loc\">Choose location</label>\n                <div className=\"map\">\n                  <SearchableMap\n                    longitude={this.onChangelong}\n                    latitude={this.onChangelat}\n                  />\n                </div>\n              </CardBody>\n\n              <Button onClick={this.onSubmit} class=\"add\">\n                Submit\n              </Button>\n            </Card>\n          </MDBCol>\n        </MDBRow>\n      </div>\n    );\n  }\n}\nexport default CreateLandAuction;\n","/home/haneen/dpm_frontend/src/Components/AddBid.js",[],"/home/haneen/dpm_frontend/src/utils/firebase.js",[],"/home/haneen/dpm_frontend/src/Components/Getbids.js",["223","224"],"import React from 'react';\nimport firebase from '../utils/firebase';\nimport '../style/MyAuctions.css'\nimport axios from 'axios';\nclass Getbids extends React.Component{\n    constructor(props){\n        super(props);\n        this.state={\n            bids:[],\n           \n         }\n        }\n        componentDidMount=()=>{\n            this.getBids();\n        }\n    getBids=()=>{\n        const bidsref = firebase.database().ref(\"Bids\");\n        bidsref.on('value',(snapshot)=>{\n            const bids = snapshot.val();\n            const list=[];\n            for(let id in bids){\n                list.push(bids[id]);\n                \n            }\n            \n            this.setState({bids:list});\n            // console.log(this.state.bids)\n        })\n        \n\n    }\n  \n    render(){\n        if(!sessionStorage.getItem('loggedIn')){\n            return <div style={{marginBottom:\"10px\",color:\"red\"}}>Login To Submit A Bid !!</div>\n        }\n        return(\n            <div>\n                \n                {this.state.bids ? this.state.bids.map((bid,index)=>{\n                    // {console.log(bid.price)}\n                   <h2>{bid.price}</h2> \n                }):\"\"\n            }\n            </div>\n        )\n    }\n}\nexport default Getbids;",["225","226"],"/home/haneen/dpm_frontend/src/Components/ViewOnMap.js",["227","228","229","230"],"import \"mapbox-gl/dist/mapbox-gl